{
  "model": "gemini-2.5-pro",
  "temperature": 0.7,
  "timeout": 600000,
  "mcpServers": {
    "cde-orchestrator": {
      "command": "python",
      "args": ["src/server.py"],
      "cwd": ".",
      "env": {
        "CDE_LOG_LEVEL": "INFO"
      },
      "timeout": 30000,
      "trust": false,
      "includeTools": [
        "cde_onboardingProject",
        "cde_scanDocumentation",
        "cde_analyzeDocumentation",
        "cde_selectWorkflow"
      ]
    }
  },
  "mcp": {
    "allowed": ["cde-orchestrator"]
  }
}
